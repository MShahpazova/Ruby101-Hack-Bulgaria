<h1>Welcome#index</h1>
<!-- <h2>Nikola was here!!!</h2> -->
<p id="notice"><%= notice %></p>

<h1>Listing Photos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <!-- <th>Url</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.name %></td>
        <td><%= photo.created_at %></td>
        <td><img src="<%= photo.url %>" alt="photo.name" width="400px" height="300px"/></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>

<%@posts.each do |post| %>
<%= post.name %>  
<% end %>





